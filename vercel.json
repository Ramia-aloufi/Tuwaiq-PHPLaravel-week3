{
    "routes": [
        {
          "src": "/(.*)",
          "dest": "/index.php"
        }
      ],
      "builds": [
        {
          "src": "public/**/*",
          "use": "@vercel/static-build"
        },
        {
          "src": "artisan",
          "use": "@vercel/php"
        }
      ]
}